{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^5.0.12",
    "@types/echarts": "^4.9.0",
    "@types/numeral": "^0.0.28",
    "@umijs/preset-react": "^1.6.8",
    "@umijs/test": "^3.2.22",
    "echarts": "^4.9.0",
    "lint-staged": "^10.0.7",
    "numeral": "^2.0.6",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-window": "^1.8.6",
    "umi": "^3.2.22",
    "umi-request": "^1.3.5",
    "yorkie": "^2.0.0"
  }
}
